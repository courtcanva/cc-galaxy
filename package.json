{
  "name": "cc-galaxy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "eslint": "eslint . --ext .ts --ext tsx --ext js",
    "prettier-check": "prettier  --check \"src/{__tests__,pages,layouts,styles,components}/**/*.{js,jsx,ts,tsx}\" ",
    "prettier-check-all": "prettier  --check .",
    "prettier-format": "prettier --write \"src/{__tests__,pages,layouts,styles,components}/**/*.{js,jsx,ts,tsx}\"",
    "prettier-format-all": "prettier --write .",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "test": "jest --colors --config jest.config.js --maxWorkers=50%",
    "test:w": "jest --colors --config jest.config.js --watch --maxWorkers=25%",
    "test:ci": "CI=true jest --colors --config jest.config.js --runInBand --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "2.3.1",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@hookform/error-message": "^2.0.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@svgr/webpack": "6.3.1",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.21.2",
    "framer-motion": "7.2.1",
    "lodash-es": "^4.17.21",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.36.1",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@testing-library/dom": "8.20.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "18.7.15",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-google": "0.14.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.0",
    "jest": "29.0.2",
    "jest-environment-jsdom": "29.0.2",
    "prettier": "^2.7.1",
    "react-icons": "4.4.0",
    "typescript": "^4.8.3"
  }
}
